<div class="columns">
  <div class="column">
    <h2 class="title is-2">Top 10 games</h2>
    <ol class="ml-5">
    <% @games.each do |game| %>
      <li>
        <%= link_to game.title, game%>
        <em>(<%= game.publisher.name%>)</em>
      </li>
    <%end%>
    </ol>
  </div>
  <div class="column">
    <h2 class="title is-2">Top 10 Publishing Companies</h2>
    <ol class="ml-5">
    <%= render @publishers %>
    </ol>
  </div>
</div>